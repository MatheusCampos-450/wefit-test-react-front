{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",

  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "modules",
    "/modules",
    "^pages/(.*)$",
    "^shared/(.*)$",
    "^[@]",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
